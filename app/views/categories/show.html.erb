<div class="container">
  <div class="row">
    <h1><%= @category.name.capitalize %></h1>
    <% @tests.each do |test| %>
      <div class="col-lg-4 col-md-6 col-12 card-category cards">
        <%= link_to test_path(test.name) do %>
          <%= test.name.upcase %>
        <% end %>
      </div>
    <% end %>
  </div>
</div>
